{
  "name": "triggerreactor-highlighter",
  "displayName": "TriggerReactor Highlighter",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "triggerreactor",
      "aliases": ["TriggerReactor", "triggerreactor"],
